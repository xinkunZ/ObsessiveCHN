{
    "name": "obsessivechn",
    "displayName": "ObsessiveCHN",
    "description": "insert whitespace between English and Chinese",
    "version": "0.1",
    "publisher": "zhangxinkun",
    "engines": {
        "vscode": "^1.11.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.obsessiveAll"
    ],
    "main": "./extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.obsessiveAll",
                "title": "%obsessiveAll%",
                "category": "%obsessive%"
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.0.3",
        "vscode": "^1.0.0",
        "mocha": "^2.3.3",
        "eslint": "^3.6.0",
        "@types/node": "^6.0.40",
        "@types/mocha": "^2.2.32"
    }
}